{
	"extends": [],
    "plugins": [],
	"env": {
		"browser": true,
		"node": true
	},
	"globals": {
		"config": true,
		"Log": true,
		"MM": true,
		"Module": true,
		"moment": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2022,
		"ecmaFeatures": {
			"globalReturn": true
		}
	},
	"rules": {
		"eqeqeq": "error",
		"no-prototype-builtins": "off",
		"no-throw-literal": "error",
		"no-unused-vars": "off",
		"no-useless-return": "error",
		"prefer-template": "error"
	}
}
